{
    "log": {
        "loglevel": "debug"
    },
    "inbounds": [
        {
            "port": {port},
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "{uuid}",
                        "alterId": 0,
                        "email": "me@example.com"
                    }
                ],
                "fallbacks": [
                    {
                        "path": "/{url}",
                        "dest": {port-internal},
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "alpn": [
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/etc/xray/cert.pem",
                            "keyFile": "/etc/xray/privkey.pem"
                        }
                    ]
                }
            }
        },
        {
            "port": {port-internal},
            "listen": "127.0.0.1",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "{uuid}",
                        "alterId": 0,
                        "email": "me@example.com"
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": true,
                    "path": "/{url}"
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ]
}